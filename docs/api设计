	添加选项:
		URL:"/api/options"
	        Method: POST
	  	Authentication: "12345685%343434"  //"userId%password"  医生登录id%医生密码
	  	Request:
		{
		  "number":"1",		//选项编号
		  "name":"test"		//选项名称
		}
		Response:
		{
    		  "status": 0,		//status  0 成功  1 失败  6 已存在
    		  "body": 2		// body 为添加所添加选项的id(主键)
		}
	修改选项信息:
		URL:"/api/options"
	        Method: PATCH
	  	Authentication: "12345685%343434"  //"userId%password"  医生登录id%医生密码
	  	Request:
		{
		  "id":"1",			//病人id
		  "number":"1",		//选项编号
		  "name":"test"		//选项名称
		}
		Response:
		}
		Response:
		{
    		  "status": 0,		//status  0 成功  1 失败  7 不存在
    		  "body": null		
		}
	删除选项:
		URL:"/api/options/{id}" 
		Method: DELETE
	  	Authentication: "12345685%343434"  //"userId%password"  医生登录id%医生密码  
		
		Response:
		{
    		  "status": 0,		//status  0 成功  1 失败  7 不存在
    		  "body": null		
		}


	根据number查询选项:				
		URL:"/api/options"
	        Method: GET
	  	Authentication: "12345685%343434"  //"userId%password"  医生登录id%医生密码
	  	URL params:
		(
		  "number":"2",	//按number查询	
		)
		Response:		
		{
    		  "status": 0,		//status  0 成功    
		  "body" :		//选项的json格式数据
    		 {			//下拉列表number与name对应关系：
           	 "id": 7,		   1       文化程度
           	 "number": 2,		   2   	心脏内造影
           	 "name": "左室",	   3	诱发方式
           	 "keyword1": null,	   4	消融术式
           	 "keyword2": null,	   5	消融径线
           	 "keyword3": null	   6	消融能源
        	},			   7	判断消融是否有效
       		 {			   8	消融终点
          	 "id": 8,		   9	并发症
           	 "number": 2,
           	 "name": "右室",
            	 "keyword1": null,
           	 "keyword2": null,
            	 "keyword3": null
      		  },
      		  {
         	  "id": 9,
         	  "number": 2,
          	  "name": "无",
           	  "keyword1": null,
          	  "keyword2": null,
          	  "keyword3": null
       		 }
  		  ]
		}

	上传文件:
		URL:"/api/file"
	        Method: POST
	  	Authentication: "12345685%343434"  //"userId%password"  医生登录id%医生密码
		Request:
		(
		  Content-Type:multipart/form-data;
		)
	  	Request:
		{
		  "file",		//所上传的文件   要求 Content-Type:multipart/form-data;
		  "interrogationRecordId":"1"		//问诊记录id
		}
		Response:
		{
    		  "status": 0,		//status  0 成功  1 失败  
    		  "body": 1_4_attachment		// body 为上传文件的保存名
		}

